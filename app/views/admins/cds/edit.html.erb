
<div class="cds">
	<%=form_for(@cd,url:artists_cd_path(@cd.id))do |f| %>
	<h2>CD情報</h2>
	<h2>CDの名前</h2>
	<%=f.text_field :cd_name %>
	<h2>ジャンル</h2>
	<%=f.text_field :genre %>
	<h2>ジャケットの写真</h2>
	<%=f.attachment_field :jacket %>
	<h2>値段</h2>
	<%=f.text_field :price %>
	<h2>レーベル名</h2>
	<%=f.text_field :label %>
	<h2>個数</h2>
	<%=f.number_field :stock%>
	<h2><%=f.submit %></h2>
	<% end %>
</div>

