<h2>User一覧</h2>
<table class="table table-condensed">
	<tr>
		<th>user名</th>
		<th>カナ名</th>
		<th>email</th>
		<th>アドレス</th>
		<th>郵便番号</th>
		<th>電話番号</th>
		<th></th>
		<th></th>
	</tr>
<% @users.each do |user| %>
<tr>
<td><%=link_to user.name,admins_user_path(user.id) %></td>
<td><%=user.kano_name %></td>
<td><%= user.email %></td>
<td><%= user.address %></td>
<td><%= user.postcode %></td>
<td><%= user.phone_number %></td>
<td><%= link_to "編集", edit_admins_user_path(user),class:"btn btn-primary" %></td>
<td><%= link_to "削除", admins_user_path(user.id), method: :delete,class:"btn btn-info","data-confirm"=>"本当に消去しますか"%></td>
</tr>
<% end %>
</table>