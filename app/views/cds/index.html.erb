	<div id="container1">
		<div id="theTarget">
			<div style="background-image: url(assets/test.jpeg)"></div>
			<div style="background-image: url(assets/test2.png)"></div>
			<div style="background-image: url(assets/test3.jpg)"></div>
			<div style="background-image: url(assets/test4.jpg)"></div>
			<div style="background-image: url(assets/test4.png)"></div>
		</div>
	</div>
	<div class="clearfix kenssaku">
		<%= form_tag root_path,class:"kennsaku clearfix",:method => 'get' do%>
		<%= text_field_tag :search, '',:placeholder => "CD名で検索..." %>
		<%= submit_tag 'Search',class:"btn btn-lg btn-block btn-base-1 mt-20 clearfix", :name => nil,style:"margin-bottom:30px;" %>
		<% end %>
	</div>
	<div class="container clearfix">
		<div class="row">
			<% @cds.each do |cd| %>
				<%if cd.status == 1 %>
					<div class="cd_container col-md-4 clearfix well">
						<%= attachment_image_tag cd, :jacket, :fill, 200, 200,fallback: "no_image.jpg" %>
						<h3>Artist名：<%= link_to cd.artist.name, every_artist_path(cd.artist_id) %></h3>
						<h3>残り:<%=cd.stock%>個</h3>
						<% if cd.stock==0 %>
							<h3>sold out</h3>
						<%else%>
							<h3>CD名：<%=link_to cd.cd_name, cd_path(cd)%></h3>
						<%end %>
						<h3>価格:<%= cd.price %>円</h3>
					</div>
				<% end %>
			<% end %>
		</div>
	</div>
